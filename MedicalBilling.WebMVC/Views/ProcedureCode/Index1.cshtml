@model IEnumerable<MedicalBilling.Models.ProcedureCodeModels.ProcedureCodeDetail>

@{
    ViewBag.Title = "Index1";
    Layout = null;
    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/bootstrap")
}

<h4>Procedure Codes</h4>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ICD10Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ICD10Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.ProcedureCodeId }) |

            </td>
        </tr>
    }

</table>

